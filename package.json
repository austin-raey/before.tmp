{
	"dependencies": {
		"next": "16.1.1",
		"react": "19.2.3",
		"react-dom": "19.2.3"
	},
	"devDependencies": {
		"@eslint/compat": "2.0.0",
		"@eslint/js": "9.39.2",
		"@tailwindcss/postcss": "4.1.18",
		"@testing-library/react": "16.3.1",
		"@types/node": "25.0.3",
		"@types/react": "19.2.7",
		"@types/react-dom": "19.2.3",
		"@vitejs/plugin-react": "5.1.2",
		"@vitest/coverage-v8": "4.0.16",
		"babel-plugin-react-compiler": "1.0.0",
		"eslint": "9.39.2",
		"eslint-config-next": "16.1.1",
		"eslint-config-prettier": "10.1.8",
		"eslint-plugin-perfectionist": "5.2.0",
		"eslint-plugin-unicorn": "62.0.0",
		"jsdom": "27.4.0",
		"lefthook": "2.0.13",
		"prettier": "3.7.4",
		"tailwindcss": "4.1.18",
		"typescript": "5.9.3",
		"typescript-eslint": "8.51.0",
		"vite-tsconfig-paths": "6.0.3",
		"vitest": "4.0.16"
	},
	"engines": {
		"bun": "1.3.5",
		"node": ">=24"
	},
	"packageManager": "bun@1.3.5",
	"private": true,
	"scripts": {
		"build": "bun --bun next build",
		"dev": "bun --bun next dev",
		"lint": "bun run next typegen && bun run lint:prettier && bun run lint:eslint && bun run lint:ts",
		"lint:eslint": "bun --bun eslint --fix",
		"lint:prettier": "bun --bun prettier --write --list-different --cache .",
		"lint:ts": "bun --bun tsc --noEmit --incremental false",
		"start": "bun --bun next start",
		"test": "bun --bun vitest run",
		"test:watch": "bun --bun vitest",
		"types": "bun --bun next typegen"
	}
}
